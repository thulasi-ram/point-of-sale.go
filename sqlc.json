{
  "version": 1,
  "packages": [
    {
      "path": "internal/db/repository",
      "queries": "./internal/db/repository/queries",
      "engine": "postgresql",
      "schema": "internal/db/migrations",
      "sql_package": "pgx/v4"
    }
  ],
  "overrides": [
    {
      "db_type": "pg_catalog.numeric",
      "go_type": "github.com/shopspring/decimal.Decimal"
    }
  ]
}